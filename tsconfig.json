{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "allowUmdGlobalAccess": true,
    "importHelpers": true,
    "allowJs": true,
      "paths": {
      "src/*": ["src/*"]
    },
  },
  "include": ["src"]
}
